{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --port 4000",
    "build": "next build && tsc -p tsconfig.build.json",
    "build:types": "tsc --noEmit",
    "start": "next start -p 4000",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules dist",
    "tidy": "rm -rf .next dist .clinic",
    "type-check": "tsc --noEmit",
    "test": "jest --maxWorkers=4 --config ../../jest.config.mjs apps/dashboard/__tests__",
    "seed:html-library": "NODE_ENV=development tsx scripts/seed-templates-cli.ts",
    "trivy:scan": "bash -lc 'set -e; REPO_ROOT=\"$(cd ../.. && pwd)\"; cd \"$REPO_ROOT\"; TRIVY_TEMP_DIR=/tmp/trivy-tmp TRIVY_CACHE_DIR=/tmp/trivy-cache; mkdir -p \"$TRIVY_TEMP_DIR\" \"$TRIVY_CACHE_DIR\"; docker build -f apps/web-base/Dockerfile -t nia-web-base:ci .; docker build -f apps/dashboard/Dockerfile -t nia-dashboard:ci --build-arg WEB_BASE_IMAGE=nia-web-base:ci .; if [ ! -f \"$TRIVY_CACHE_DIR/trivy/db/trivy.db\" ]; then docker run --rm -v /var/run/docker.sock:/var/run/docker.sock -v \"$TRIVY_CACHE_DIR\":/root/.cache -v \"$TRIVY_TEMP_DIR\":/tmp aquasecurity/trivy:0.65.0 image --download-db-only --cache-dir /root/.cache; fi; docker run --rm -v /var/run/docker.sock:/var/run/docker.sock -v \"$TRIVY_CACHE_DIR\":/root/.cache -v \"$TRIVY_TEMP_DIR\":/tmp aquasecurity/trivy:0.65.0 image --severity CRITICAL,HIGH --pkg-types os,library --scanners vuln --skip-db-update --skip-java-db-update --cache-dir /root/.cache --timeout 10m --exit-code 1 nia-dashboard:ci'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.823.0",
    "@aws-sdk/s3-request-presigner": "^3.823.0",
    "@emotion/is-prop-valid": "^1.2.2",
    "@hookform/resolvers": "^3.10.0",
    "@nia/events": "*",
    "@nia/features": "*",
    "@nia/prism": "*",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@slonik/driver": "^47.3.2",
    "@types/jszip": "^3.4.0",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cypress": "^14.5.4",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "dotted-map": "^2.2.3",
    "ejs": "^3.1.10",
    "framer-motion": "^11.11.17",
    "husky": "^9.1.7",
    "jose": "^5.9.6",
    "jszip": "^3.10.1",
    "lucide-react": "^0.461.0",
    "motion": "^11.11.17",
    "msw": "2.10.2",
    "next": "^15.5.9",
    "next-themes": "^0.4.3",
    "papaparse": "^5.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.0.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.21",
    "sass": "^1.72.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.3.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "qs": "6.14.1"
  }
}
